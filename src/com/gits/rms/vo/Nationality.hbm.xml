<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="com.gits.rms.vo.NationalityVO" table="hcmo_nationality">
     <id
        name="nationalityId"
        type="integer"
        column="HcmoNationalityId"
        length="11"
       >
        <generator class="native" />
    </id>
    
     <property
        name="nationalityName"
        type="string"
        column="NatName"
        length="120"
    />
   
    <property
        name="isActive"
        type="integer"
        column="IsActive"
        length="1"
    />

     </class>
</hibernate-mapping>