<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.gits.rms.vo.CountryVO" table="hcmo_country">
     <id
        name="hcmocountryId"
        type="integer"
        column="HcmoCountryId"
        length="11"
       >
        <generator class="native" />
    </id>
     <property
        name="countryName"
        type="string"
        column="Name"
        length="100"
    />
    <property
        name="countryCode"
        type="string"
        column="CountryCode"
        length="60"
    />
    <property
        name="description"
        type="text"
        column="Description"
       />
       
   
    <property
        name="isActive"
        type="integer"
        column="IsActive"
        length="1"
    />
	
</class>
</hibernate-mapping>
