<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.gits.rms.vo.OrganizationTypeVO" table="hcmo_org_type">
     <id
        name="hcmoorgTypeId"
        type="integer"
        column="HcmoOrgTypeId"
        length="11"
       >
        <generator class="native" />
    </id>
    <property 
     name="clientId"
     type="integer"
     column="HcmoClientId"
     length="11" 
     />
     <property
        name="orgTypeName"
        type="string"
        column="Name"
        length="60"
    />
     <property
        name="description"
        type="text"
        column="Description"
       />
	<property
        name="created"
        type="date"
        column="Created"
    />
     <property
        name="updated"
        type="timestamp"
        column="Updated"
    />
    <property
        name="isActive"
        type="integer"
        column="IsActive"
        length="1"
    />
    

<many-to-one name="createdBy" column="CreatedBy" lazy="false" class="com.gits.rms.vo.EmployeesVO"/>
<many-to-one name="updatedBy" column="UpdatedBy" lazy="false" class="com.gits.rms.vo.EmployeesVO"/>
    
     </class>
</hibernate-mapping>