<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
    "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
    
<validators>
 
<field name="license.empIdObj.employeeId">
  	<field-validator type="required">
   		<message key="errors.empName"></message>
  	</field-validator>
</field>
 
<field name="license.licenseNumber">
    <field-validator type="requiredstring">
	  	<param name="trim">true</param>
   		<message key="errors.license.licNumber"></message>
    </field-validator>
    <field-validator type="stringlength">
  	    <param name="maxLength">100</param>
  	    <message key="errors.length.license.licNumber"></message>
    </field-validator>
</field>
 
<field name="license.licenseDate">
    <field-validator type="required">
   		<message key="errors.license.date"></message>
    </field-validator>
    <field-validator type="date">
	    <param name="min">1/2/1970</param>
	    <message key="errors.license.date.invalid"></message>
    </field-validator>
</field>
 
<field name="license.licenseRenewalDate">
    <field-validator type="required">
   		<message key="errors.license.renewalDate"></message>
    </field-validator>
    <field-validator type="date">
	    <param name="min">1/2/1970</param>
	    <message key="errors.license.renewalDate.invalid"></message>
	</field-validator>
</field>

<field name="license.licenseDesc">
    <field-validator type="requiredstring">
   		<param name="trim">true</param>
   		<message key="errors.license.description"></message>
    </field-validator>
</field>
 
</validators>