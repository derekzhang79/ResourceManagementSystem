<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
    "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
    
<validators>
 
<field name="empSpace.sharedFileTitle">
  <field-validator type="requiredstring">
   		<param name="trim">true</param>
   		<message key="errors.required.empSpace.title"></message>
  </field-validator>
  <field-validator type="stringlength">
   		<param name="maxLength">100</param>
   		<message key="errors.length.empSpace.title"></message>
  </field-validator>
</field>

<field name="empSpace.sharedFileDesc">
  <field-validator type="requiredstring">
   		<param name="trim">true</param>
   		<message key="errors.required.empSpace.description"></message>
  </field-validator>
</field>
 
</validators>