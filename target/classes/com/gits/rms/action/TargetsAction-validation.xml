<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
    "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
    
<validators>
	<field name="target.targetName">
	  	<field-validator type="requiredstring" short-circuit="true">
	   		<param name="trim">true</param>
	   		<message key="errors.target.name"></message>
	  	</field-validator>
	  	
	  	<field-validator type="stringlength" short-circuit="true">
	        <param name="maxLength">120</param>
	        <param name="trim">true</param>
	        <message key="errors.length.target.name"/>
	   	</field-validator>
 	</field>
	<field name="target.targetTypeObj.hcmoTargetTypeId">
	 	<field-validator type="required">
	  		<message key="errors.target.type"></message>
	 	</field-validator>
	</field>
	<field name="target.targetMode">
  		<field-validator type="requiredstring">
  			<param name="trim">true</param>	
   			<message key="errors.target.mode"></message>
  		</field-validator>
 	</field>
  	<field name="target.targetValue">
  		<field-validator type="requiredstring">
  			<param name="trim">true</param>	
   			<message key="errors.target.value"></message>
  		</field-validator>
 	</field>
</validators>